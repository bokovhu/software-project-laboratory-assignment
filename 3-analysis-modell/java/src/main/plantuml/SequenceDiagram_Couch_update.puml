@startuml

skinparam classAttributeIconSize 0
skinparam guillemet false

participant "c : Couch" as c
participant "w : Wave" as w
participant "t : Tile" as t

[-> c : update ()
activate c

c -> w ** : new

c -> w ++ : setType (BEEPING)
w --> c --

c -> c ++ : getPlacedOn ()
c --> c -- : t

c -> t ++ : spawnWave (w)
t --> c --

[<-- c
deactivate c

@enduml